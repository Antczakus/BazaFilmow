<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane id="BorderPane" prefHeight="400.0" prefWidth="600.0" style="-fx-border-width: 4;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bazafilmow.MainController">
   <center>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="newMovie" mnemonicParsing="false" onAction="#handleButtonAction" prefWidth="200.0" text="New Film">
               <font>
                  <Font size="24.0" />
               </font>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" prefWidth="200.0" text="Edit Film">
               <font>
                  <Font size="24.0" />
               </font>
               <VBox.margin>
                  <Insets top="50.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" prefWidth="200.0" text="Delete Film">
               <font>
                  <Font size="24.0" />
               </font>
               <VBox.margin>
                  <Insets top="50.0" />
               </VBox.margin>
            </Button>
         </children>
         <BorderPane.margin>
            <Insets left="200.0" right="200.0" top="50.0" />
         </BorderPane.margin>
      </VBox>
   </center>
   <left>
      <Button id="Sz1" fx:id="SzukajAktora" mnemonicParsing="false" onAction="#GoSzukaj" prefHeight="31.0" prefWidth="64.0" text="SzukajAktora" BorderPane.alignment="CENTER" />
   </left>
</BorderPane>
